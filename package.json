{
  "name": "@satisgraphtory/tool-belt",
  "version": "1.0.0",
  "description": "Data processing toolchain for SatisGraphtory",
  "repository": "https://github.com/SatisGraphtory/ToolBelt",
  "author": "tehalexf",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": {
    "packages": [
      "pipeline/*"
    ]
  },
  "scripts": {
    "pak-parser": "ts-node --project ./pipeline/pak-tools/tsconfig.json ./pipeline/pak-tools/bin/pak-parser",
    "headers-to-interfaces": "ts-node --project ./pipeline/headers-to-interfaces/tsconfig.json ./pipeline/headers-to-interfaces/main",
    "interfaces-to-json": "ts-node --project ./pipeline/interfaces-to-json/tsconfig.json ./pipeline/interfaces-to-json/main",
    "checkout-source-data": "./scripts/checkout.sh SourceData",
    "checkout-data-landing": "./scripts/checkout.sh DataLanding"
  },
  "dependencies": {
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "yarn-run-all": "^3.1.1"
  }
}
